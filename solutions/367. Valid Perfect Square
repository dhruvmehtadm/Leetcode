class Solution:
    def isPerfectSquare(self, num: int) -> bool:
        #this method is also correct
        '''
        n = int(num**0.5)
        n2 = n**2
        if(n2==num):
            return True
        else:
            return False
        '''
        i = 1
        while (num>0):
            num -= i
            if(num==0):
                return True
            else:
                i += 2       
        return False
